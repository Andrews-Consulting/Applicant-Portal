<aura:component extends="c:dx_StdPageAbstraction" implements="forceCommunity:availableForAllPageTypes,force:hasRecordId" access="global">
    <aura:set attribute="ActiveCmp" value="dx_PC_ApplicationQuestions_Premise" /> <!-- This drives the navigation and needs to exist in Nav metadata -->
    <div aura:id="dx_PC_ApplicationQuestions_PremiseDiv">

 	    <div class="slds-grid slds-wrap padded">
            <div class="slds-col slds-size--1-of-1 slds-medium-size--2-of-12" />
            <div class="slds-col slds-size--1-of-1 slds-medium-size--10-of-12">
                <h1>Additional Questions related to the Premises</h1>
            </div>
    	</div>
    <br />
		
    <c:dx_checklistDisplaySectionOnly Section="Premises Information" SaveCompleted="{!c.SaveComplete}" EmptyComponent="{!c.EmptyComponent}" showError="{!v.showError}"  errorMessage="{!v.errorMessage}" aura:id="dx_PC_ApplicationQuestions_Premise" recordId="{!v.recordId}" RecordIdIsApplication="{!v.RecordIdIsApplication}" isInitComplete="{! v.InitCompleteTriggerFlag}" /> 

    <c:dx_checklistDisplaySectionOnly Section="General Premises Information" SaveCompleted="{!c.SaveComplete}" EmptyComponent="{!c.EmptyComponent}" showError="{!v.showError}" errorMessage="{!v.errorMessage}" aura:id="dx_PC_ApplicationQuestions_Premise" recordId="{!v.recordId}" RecordIdIsApplication="{!v.RecordIdIsApplication}" isInitComplete="{! v.InitCompleteTriggerFlag}"/> 

    <c:dx_checklistDisplaySectionOnly Section="Auction" SaveCompleted="{!c.SaveComplete}" EmptyComponent="{!c.EmptyComponent}" showError="{!v.showError}" errorMessage="{!v.errorMessage}" aura:id="dx_PC_ApplicationQuestions_Premise" recordId="{!v.recordId}" RecordIdIsApplication="{!v.RecordIdIsApplication}" isInitComplete="{! v.InitCompleteTriggerFlag}"/> 

    </div>

</aura:component>